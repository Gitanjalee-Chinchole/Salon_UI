{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/spec",
    "baseUrl": "./",
    "module": "commonjs",
    "target": "es5",
    "types": [
      "jasmine",
      "node"
    ]
  },
  "files": [
    "test.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts"
  ],
  "exclude": [
    "./app/starter/starter-content/starter-content.component.ts",
    "./app/starter/starter-control-sidebar/*",
    "./app/starter/starter-footer/*",
    "./app/starter/starter-header/*",
    "./app/starter/starter-left-side/*"
  ],
  "paths": {
    "@angular/*": ["../node_modules/@angular/*"]
  }
}
